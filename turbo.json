{
  "$schema": "https://turborepo.com/schema.v2.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^builld"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": [],
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
